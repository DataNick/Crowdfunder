<h1>Projects#new</h1>

<%= form_for @project do |f| %>

	<div class="field">
		<%= f.label :title %><br/>
		<%= f.text_field :title %>
	</div>

	<div class="field">
		<%= f.label :goal %><br/>
		<%= f.text_field :goal %>
	</div>

	<div class="field">
		<%= f.label :description %><br/>
		<%= f.password_field :description %>
	</div>

	<div class="field">
		<%= f.label :start_date %><br/>
		<%= f.password_field :start_date %>
	</div>

	<div class="field">
		<%= f.label :end_date %><br/>
		<%= f.password_field :end_date %>
	</div><br/>

	<div class="field">
		<%= f.submit "Submit"%>
	</div>

<% end %>
